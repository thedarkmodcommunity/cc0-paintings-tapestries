welli_banner12
{

	surftype15
	description "cloth"

	{
		blend	diffusemap
		map	models/darkmod/props/textures/wellibanner12_d
		alphaTest 0.5
	}

	bumpmap		models/darkmod/props/textures/banner3_local
	specularmap	models/darkmod/props/textures/banner3_s
    {
        if ( parm11 > 0 )
        blend       gl_dst_color, gl_one
        map         _white
        rgb         0.40 * parm11
    }
    {
        if ( parm11 > 0 )
        blend       add
        map         models/darkmod/props/textures/wellibanner12_d
        rgb         0.15 * parm11
    }
}

